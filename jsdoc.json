{
  "source": {
    "include": ["./src", "./index.js", "./src/cli", "./src/cli/commands.js", "./src/cli/shell.js"]
  },
  "opts": {
    "destination": "./docs",
    "template": "../../../../../Sandbox/dev_tools/assets/better-docs",
    "readme": "./README.md"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "search": true,
    "default": {
      "staticFiles": {
        "include": ["./README.md"]
      }
    },
    "better-docs": {
      "name": "This.Me",
      "title": "This.Me",
      "css": "style.css",
      "trackingCode": "...",
      "hideGenerator": false,
      "navLinks": [
        {
          "label": "All.This",
          "href": "https://suign.github.io/all.this/"
        },
        {
          "label": "Github",
          "href": "https://github.com/suiGn/neurons.me"
        },
        {
          "label": "App",
          "href": "https://neurons.me"
        }
      ]
    }
  }
}
